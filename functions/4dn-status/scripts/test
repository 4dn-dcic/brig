if [ $# -ne 1 ]; then
  echo "Syntax: $0 <directory>"
  exit 1
fi

dirname="$1"

if [ "${dirname:~0}" != "/" ]; then

    # Add a slash to dirname if one was not given.
    dirname="${dirname}/"

fi


testfile="${dirname}lambda_function.py"

echo "Running test file ${testfile} ..."

python "${testfile}"
